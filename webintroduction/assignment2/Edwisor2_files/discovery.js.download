let getDomain = () => {
    let hostName = window.location.host.split('.')
    if (hostName[1] === 'testedwisor') {
        return 'testedwisor';
    } else if (hostName[1] === 'edwisor') {
        return 'edwisor';
    } else {
        return 'testedwisor';
    }

}

